//배열
#include<stdio.h>
int main(){
	int i,a[5];
	for(i=0;i<5;i++){
		scanf("%d",&a[i]);
	}
	for(i=0;i<5;i++){
		printf("%d ",a[i]);
	}
	
	return 0;
}


#include<stdio.h>
int main(){
	int a[5]= {30,40,20};
	int i;
	a[3] = 80;
	for(i=0;i<5;i++){
		printf("a[%d]=%d\n",i,a[i]);
	}
}

#include<stdio.h>
int main(){
	char a[5]= {'m','i','c','n'};
	int i;
	
	printf("%d",a[4]);
	a[5] = 10;
	for(i=0;i<5;i++){
		printf("%c",a[i]);
	}
	
}
