#include <stdio.h>
int main(){
	int a[10] = {0};
	int k,i;
	for(i=0;i<10;i++){
		scanf("%d",&a[i]);
	}
	scanf("%d",&k);
	printf("%d",a[k-1]);
}


#include <stdio.h>
int main(){
	
	int n,i,j;
	
	scanf("%d",&n);
	int a[n] = {0};
	
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
	for(j=n;j>0;j--){
		printf("%d ",a[j-1]);
	}
	
}

#include <stdio.h>
int main(){
	int bit[64]={0}, i=0,j,n;
	scanf("%d",&n);
	
	if (n==0) printf("0");
	else{
		for(i=0;i<99;i++){
			if(n==1){
				bit[i]=1;
				
				break;
			}
			bit[i]=n%2;
			n = n/2;
			
		}

		for(j=i;j>=0;j--){
			printf("%d",bit[j]);
		}
	}
	
}

#include <stdio.h>
int main(){
	int bit[64]={0}, i=0,j,n;
	scanf("%d",&n);
	
	if (n==0) printf("0");
	else{
		for(i=0;i<99;i++){
			if(n==1){
				bit[i]=1;
				
				break;
			}
			bit[i]=n%2;
			n = n/2;
			
		}

		for(j=i;j>=0;j--){
			printf("%d",bit[j]);
		}
	}
	
}
